{
    "version": 3,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 21,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "clang",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER": "clang",
                "CMAKE_CXX_COMPILER": "clang++"
            }
        },
        {
            "name": "core",
            "displayName": "core",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "LTO": true,
                "SINGLE_FILE": true
            }
        },
        {
            "name": "core-dev",
            "displayName": "core-dev",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "GBA_DEV": true
            }
        },
        {
            "name": "core-table",
            "displayName": "core-table",
            "inherits": ["core"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_TABLE"
            }
        },
        {
            "name": "core-switch",
            "displayName": "core-switch",
            "inherits": ["core"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_SWITCH"
            }
        },
        {
            "name": "core-goto",
            "displayName": "core-goto",
            "inherits": ["core"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_GOTO"
            }
        },
        {
            "name": "imgui",
            "displayName": "imgui",
            "inherits": ["core"],
            "cacheVariables": {
                "IMGUI": true
            }
        },
        {
            "name": "imgui-dev",
            "displayName": "imgui-dev",
            "inherits": ["core-dev"],
            "cacheVariables": {
                "IMGUI": true
            }
        },
        {
            "name": "vcpkg",
            "displayName": "vcpkg",
            "inherits": ["imgui"],
            "cacheVariables": {
                "VCPKG": true
            }
        },
        {
            "name": "vcpkg-dev",
            "displayName": "vcpkg-dev",
            "inherits": ["imgui-dev"],
            "cacheVariables": {
                "VCPKG": true
            }
        },
        {
            "name": "sdl2",
            "displayName": "sdl2",
            "inherits": ["core"],
            "cacheVariables": {
                "SDL2": true
            }
        },
        {
            "name": "sdl2-dev",
            "displayName": "sdl2-dev",
            "inherits": ["core-dev"],
            "cacheVariables": {
                "SDL2": true
            }
        },
        {
            "name": "vcpkg-sdl2",
            "displayName": "vcpkg-sdl2",
            "inherits": ["sdl2"],
            "toolchainFile": "$env{VCPKG_INSTALLATION_ROOT}/scripts/buildsystems/vcpkg.cmake"
        },
        {
            "name": "vcpkg-sdl2-dev",
            "displayName": "vcpkg-sdl2-dev",
            "inherits": ["sdl2-dev"],
            "toolchainFile": "$env{VCPKG_INSTALLATION_ROOT}/scripts/buildsystems/vcpkg.cmake",
        },
        {
            "name": "emsdk",
            "displayName": "emsdk",
            "inherits": ["core"],
            "toolchainFile": "$env{EMSDK}/upstream/emscripten/cmake/Modules/Platform/Emscripten.cmake"
        },
        {
            "name": "emsdk-dev",
            "displayName": "emsdk-dev",
            "inherits": ["core-dev"],
            "toolchainFile": "$env{EMSDK}/upstream/emscripten/cmake/Modules/Platform/Emscripten.cmake",
            "cacheVariables": {
                "EMRUN": true
            }
        },
        {
            "name": "benchmark",
            "displayName": "benchmark",
            "inherits": ["core"],
            "cacheVariables": {
                "BENCHMARK": true
            }
        },
        {
            "name": "benchmark-dev",
            "displayName": "benchmark-dev",
            "inherits": ["core-dev"],
            "cacheVariables": {
                "BENCHMARK": true
            }
        },
        {
            "name": "sdl2-table",
            "displayName": "sdl2-table",
            "inherits": ["sdl2"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_TABLE"
            }
        },
        {
            "name": "sdl2-switch",
            "displayName": "sdl2-switch",
            "inherits": ["sdl2"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_SWITCH"
            }
        },
        {
            "name": "sdl2-goto",
            "displayName": "sdl2-goto",
            "inherits": ["sdl2"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_GOTO"
            }
        },
        {
            "name": "benchmark-table",
            "displayName": "benchmark-table",
            "inherits": ["benchmark"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_TABLE",
                "LTO": false
            }
        },
        {
            "name": "benchmark-switch",
            "displayName": "benchmark-switch",
            "inherits": ["benchmark"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_SWITCH",
                "LTO": false
            }
        },
        {
            "name": "benchmark-goto",
            "displayName": "benchmark-goto",
            "inherits": ["benchmark"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_GOTO",
                "LTO": false
            }
        },
        {
            "name": "benchmark-table-lto",
            "displayName": "benchmark-table-lto",
            "inherits": ["benchmark"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_TABLE"
            }
        },
        {
            "name": "benchmark-switch-lto",
            "displayName": "benchmark-switch-lto",
            "inherits": ["benchmark"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_SWITCH"
            }
        },
        {
            "name": "benchmark-goto-lto",
            "displayName": "benchmark-goto-lto",
            "inherits": ["benchmark"],
            "cacheVariables": {
                "INTERPRETER": "INTERPRETER_GOTO"
            }
        },
        {
            "name": "benchmark-clang-table",
            "displayName": "benchmark-clang-table",
            "inherits": ["benchmark-table", "clang"]
        },
        {
            "name": "benchmark-clang-switch",
            "displayName": "benchmark-clang-switch",
            "inherits": ["benchmark-switch", "clang"]
        },
        {
            "name": "benchmark-clang-goto",
            "displayName": "benchmark-clang-goto",
            "inherits": ["benchmark-goto", "clang"]
        },
        {
            "name": "benchmark-clang-table-lto",
            "displayName": "benchmark-clang-table-lto",
            "inherits": ["benchmark-table-lto", "clang"]
        },
        {
            "name": "benchmark-clang-switch-lto",
            "displayName": "benchmark-clang-switch-lto",
            "inherits": ["benchmark-switch-lto", "clang"]
        },
        {
            "name": "benchmark-clang-goto-lto",
            "displayName": "benchmark-clang-goto-lto",
            "inherits": ["benchmark-goto-lto", "clang"]
        }
    ]
}
